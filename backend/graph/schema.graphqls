# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  todos: [Todo!]!
}

input userCreate {
  userID: String
  userFName: String!
  userLName: String!
}

type Mutation {
  createUser(input: userCreate!): Todo!
}
