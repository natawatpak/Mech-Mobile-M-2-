input carUpdateInput {
  ID: ID!
  ownerID: ID!
  plateNum: String!
  plateType: String
  issuedAt: String
  color: String
  type: String
  brand: String!
  build: String
}

type ticket {
  ID: ID!
  customerID: ID!
  carID: ID!
  problem: String!
  description: String
  createTime: Time!
  shopID: ID
  acceptedTime: Time
  status: String
  longitude: Float!
  latitude: Float!
